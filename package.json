{
  "name": "blog-jekyll",
  "version": "2.0.0-alpha",
  "description": "![home](./assets/img/home.png)",
  "scripts": {
    "dev": "npm run jekyll:dev",
    "clean": "rm -rf _site/",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run js:build && npm run jekyll:build ",
    "build:types": "tsc --emitDeclarationOnly",
    "js:build": "webpack",
    "jekyll:build": "bundle exec jekyll build",
    "jekyll:install": "bundle install",
    "jekyll:dev": "bundle exec jekyll serve",
    "postinstall": "jekyll:install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonesrussell/blog.git"
  },
  "keywords": [],
  "author": "Russell Jones <jonesrussell42@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonesrussell/blog/issues"
  },
  "homepage": "https://github.com/jonesrussell/blog#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@sentry/browser": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "analytics": "^0.6.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "fork-ts-checker-webpack-plugin": "^6.0.7",
    "logrocket": "^1.0.14",
    "mini-css-extract-plugin": "^1.3.3",
    "mini-svg-data-uri": "^1.2.3",
    "purgecss-webpack-plugin": "^3.1.3",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {}
}

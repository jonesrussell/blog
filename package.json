{
  "name": "blog-jekyll",
  "version": "2.0.0-alpha",
  "description": "![home](./assets/img/home.png)",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "postinstall": "bundle install",
    "start": "bundle exec jekyll serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonesrussell/blog.git"
  },
  "keywords": [],
  "author": "Russell Jones <jonesrussell42@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonesrussell/blog/issues"
  },
  "homepage": "https://github.com/jonesrussell/blog#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "babel-loader": "^8.2.2",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.29.2",
    "@sentry/tracing": "^5.29.2"
  }
}
